<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <style name="Theme.GEUOne" parent="Theme.Material3.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#AD5E74</item>
        <item name="colorPrimaryVariant">#742E46</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">#AD5E74</item>
        <item name="colorSecondaryVariant">#742E46</item>
        <item name="colorOnSecondary">@color/white</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>

        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="MyLinearTheme">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">#91475d</item>
    </style>

    <style name="Theme.GEUOne.AppBarOverlay">
        <item name="android:tint">#FAD4E1</item>
        <item name="colorPrimary">#ad5e74</item>
        <item name="contentScrim">#742e46</item>
    </style>

    <style name="MyCard" parent="CardView">
        <item name="cardCornerRadius">8dp</item>
        <item name="cardBackgroundColor">@android:color/transparent</item>
    </style>

</resources>